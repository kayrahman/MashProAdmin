<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation_creator"
    app:startDestination="@+id/uploadFragment">


    <fragment
        android:id="@+id/uploadFragment"
        android:name="com.nkr.mashpro.ui.upload.UploadFragment"
        android:label="Upload" />

    <fragment
        android:id="@+id/moviePlayerFragment2"
        android:name="com.nkr.mashpro.ui.moviePlayer.MoviePlayerFragment"
        android:label="movie_player_fragment"
        tools:layout="@layout/movie_player_fragment" >
        <argument
            android:name="movie"
            app:argType="com.nkr.mashpro.model.Movie" />
    </fragment>
    <fragment
        android:id="@+id/creatorAccountFragment"
        android:name="com.nkr.mashpro.ui.account.CreatorAccountFragment"
        android:label="CreatorAccountFragment" >
        <action
            android:id="@+id/action_creatorAccountFragment_to_moviePlayerFragment2"
            app:destination="@id/moviePlayerFragment2" />
    </fragment>
</navigation>